{
  "name": "react-uikit-img-gallery",
  "version": "1.0.1",
  "description": "React uikit image gallery component.",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react",
    "img-group"
  ],
  "repository": "git@github.com:otissv/react-uikit-img-gallery.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./lib/img-gallery.js",
  "scripts": {
    "build": "babel -d lib/ src/ && date",
    "build:watch": "watch 'npm run build' ./src",
    "lint": "eslint --quiet ./src/** && date  || true && date",
    "lint:watch": "watch 'npm run lint' ./",
    "peer": "npm install cuid ufunc react react-dom react-uikit-base react-uikit-img",
    "prepublish": "babel -d lib/ src/ && date",
    "test": "babel-node test-runner.js | tap-min && date || true && date",
    "test:spec": "babel-node test-runner.js | tap-difflet && date || true && date",
    "test:watch": "watch 'npm run test' ./"
  },
  "devDependencies": {
    "babel-cli": "~6.11.4",
    "babel-core": "~6.13.2",
    "babel-eslint": "~6.1.2",
    "babel-plugin-transform-runtime": "~6.12.0",
    "babel-preset-es2015": "~6.13.2",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-0": "~6.5.0",
    "babel-runtime": "~6.11.6",
    "blue-tape": "~0.2.0",
    "cheerio": "~0.20.0",
    "cuid": "~1.3.8",
    "eslint": "~3.3.0",
    "eslint-config-standard": "~5.3.0",
    "eslint-plugin-react": "~6.0.0",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~2.0.0",
    "eslint-plugin-promise": "~2.0.1",
    "faucet": "~0.0.1",
    "glob": "~7.0.3",
    "history": "~3.0.0",
    "react-addons-test-utils": "~15.3.0",
    "tap-difflet": "~0.6.0",
    "tap-min": "~1.1.0",
    "watch": "~0.19.1"
  },
  "dependencies": {
    "react": "~15.3.0",
    "react-dom": "~15.3.0",
    "react-uikit-base": "~1.0.5",
    "react-uikit-img": "~1.0.1",
    "ufunc": "~1.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": "~4.4.3"
  }
}
